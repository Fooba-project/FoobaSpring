<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="fooba.dao.IMemberDao3">
	<resultMap id="memberMap3" type="java.util.HashMap"></resultMap>

	<insert  id="writeReview" parameterType="fooba.dto.ReviewVO">
		insert into review(review_seq,id,rseq,star,image,content,oseq,nick)
		values(review_seq_seq.nextVal,#{ID},#{RSEQ},#{STAR},#{IMAGE},#{CONTENT},#{OSEQ},#{NICK})
	</insert>
	
	<update id="reviewComplete" >
		update orders set result=3 where oseq=#{param1}
	</update>
	
</mapper>