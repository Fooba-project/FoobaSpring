<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="fooba.dao.IMemberDao2">
	<resultMap id="memberMap2" type="java.util.HashMap"></resultMap>

	<select id="getMember"  statementType="CALLABLE" parameterType="java.util.HashMap">
		{	CALL getMemberFooba(#{ID},
				#{ref_cursor ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 } )	}
	</select>	

	<select id="SearchResList" statementType="CALLABLE" parameterType="java.util.HashMap">
		{	CALL SearchResList(#{searchtext},
		#{ref_cursor ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>

	<select id="FimagebyRseq" statementType="CALLABLE" parameterType="java.util.HashMap">
		{	CALL FimagebyRseq(#{RSEQ},
		#{ref_cursor ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>
	
	<select id="searchKind" statementType="CALLABLE" parameterType="java.util.HashMap">
		{	CALL searchKind(#{KIND},
		#{ref_cursor ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>
	
	<select id="resInf" statementType="CALLABLE" parameterType="java.util.HashMap">
	{	CALL resInf(#{RSEQ},
	#{ref_cursor1 ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>
	
	<select id="reviewList" statementType="CALLABLE" parameterType="java.util.HashMap">
	{ CALL reviewList(#{RSEQ},
	#{ref_cursor2 ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>
	
	<select id="cartList" statementType="CALLABLE" parameterType="java.util.HashMap">
		{ CALL cartList(#{RSEQ}, #{ID},
	#{ref_cursor3 ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>
	
	<select id="getFoodDetail" statementType="CALLABLE" parameterType="java.util.HashMap">
		{CALL getFoodDetail(#{FSEQ},
		#{ref_cursor ,mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=memberMap2 })}
	</select>
	
	<insert id="insertCart" statementType="CALLABLE" parameterType="fooba.dto.CartVO">
	{CALL insertCart(#{cvo.ID}, #{cvo.FSEQ},#{cvo.SIDEYN1},#{cvo.SIDEYN2},#{cvo.SIDEYN3},#{cvo.QUANTITY},#{cvo.CPRICE},#{cvo.CFNAME},#{cvo.RSEQ})}
	</insert>
</mapper>